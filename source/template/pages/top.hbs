---
title: top
layout: default.hbs
side: false
---
	<div class="grid">
		<div class="l-main">
			<div class="u-pickup">
				<h2 class="heading heading--a">Pickup</h2>
				<div class="block block--3 block--margin fz-30">
					{{#each post_item3}}
					<div class="block__item">
						<div class="article fz-16">
							<div class="article__header">
								<div class="article__thumb">
									<img src="{{{pic}}}/640/350">
								</div>
							</div>
							<div class="article__body">
								<p class="article__body__title">{{title}}</p>
								<!-- <p class="article__body__data">{{{date_time}}}</p> -->
								<!-- <p class="article__body__action">
									<i class="icon-wf" data-icon-bf="&#xE600"></i>
								</p> -->
							</div>
						</div>
					</div>
					{{/each}}
				</div>
			</div>
			<div class="u-recently-Post">
				<h2 class="heading heading--a">RecentlyPost</h2>
				<div class="block block--4 block--margin">
					{{#each post_item10}}
					<div class="block__item">
						<div class="article">
							<div class="article__header">
								<div class="article__thumb">
									<img src="{{{pic}}}/307/210">
								</div>
							</div>
							<div class="article__body">
								<p class="article__body__title">{{title}}</p>
								<!-- <p class="article__body__data">{{{date_time}}}</p> -->
								<!-- <p class="article__body__action">
									<i class="icon-wf" data-icon-bf="&#xE600"></i>
								</p> -->
							</div>
						</div>
					</div>
					{{/each}}
				</div>
			</div>
		</div>
	</div>